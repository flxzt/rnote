<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="@APP_ID@" path="@APP_IDPATH@">
    <key name="window-width" type="i">
      <default>-1</default>
      <summary>Default window width</summary>
    </key>
    <key name="window-height" type="i">
      <default>-1</default>
      <summary>Default window height</summary>
    </key>
    <key name="is-maximized" type="b">
      <default>false</default>
      <summary>Default window maximized behaviour</summary>
    </key>
    <key name="flap-width" type="i">
      <default>500</default>
      <summary>flap width</summary>
    </key>
    <key name='color-scheme' type='s'>
      <choices>
        <choice value="default" />
        <choice value="force-light" />
        <choice value="force-dark" />
      </choices>
      <default>"default"</default>
      <summary>Color scheme</summary>
      <description>
        Use the color scheme in the application's UI and in the text area.
      </description>
    </key>
    <key name="current-workspace-index" type="u">
      <default>0</default>
      <summary>the current workspace index</summary>
    </key>
    <key name="workspaces" type="as">
      <default>["./"]</default>
      <summary>the workspaces</summary>
    </key>
    <key name="autosave" type="b">
      <default>true</default>
      <summary>true when autosave is enabled</summary>
    </key>
    <key name="autosave-interval-secs" type="u">
      <default>120</default>
      <summary>the sec interval for the autosave</summary>
    </key>
    <key name="righthanded" type="b">
      <default>true</default>
      <summary>Wether the user is righthanded (or lefthanded)</summary>
    </key>
    <key name="touch-drawing" type="b">
      <default>false</default>
      <summary>enable drawing with touch input</summary>
    </key>
    <key name="brushpage-selected-color" type="u">
      <default>0</default>
      <summary>the default selected color row on the brush page</summary>
    </key>
    <key name="brushpage-colors" type="(uuuuuuuu)">
      <default>(
        0x000000ff,
        0xffffffff,
        0x99c1f1ff,
        0x1a5fb4ff,
        0x26a269ff,
        0xf6d32dff,
        0xff7800ff,
        0xe01b24ff
      )</default>
      <summary>the brush page colors</summary>
    </key>
    <key name="shaperpage-selected-color" type="u">
      <default>0</default>
      <summary>the default selected color row on the shaper page</summary>
    </key>
    <key name="shaperpage-colors" type="(uu)">
      <default>(
        0x000000ff,
        0x1a5fb4ff
      )</default>
      <summary>the shaper page colors</summary>
    </key>
    <key name="shaperpage-selected-fill" type="u">
      <default>0</default>
      <summary>the default selected fill color row on the shaper page</summary>
    </key>
    <key name="shaperpage-fills" type="(uu)">
      <default>(
        0x00000000,
        0x99c1f1ff
      )</default>
      <summary>the shaper page fill colors</summary>
    </key>
    <key name="typewriterpage-colors" type="(uu)">
      <default>(
        0x000000ff,
        0x1a5fb4ff
      )</default>
      <summary>the typewriter page colors</summary>
    </key>
    <key name="typewriterpage-selected-color" type="u">
      <default>0</default>
      <summary>the default selected color row on the typewriter page</summary>
    </key>
    <key name="resize-lock-aspectratio" type="b">
      <default>false</default>
      <summary>if the aspect ratio while resizing is locked</summary>
    </key>
    <key name="engine-config" type="s">
      <default>''</default>
      <summary>the engine configuration</summary>
    </key>
  </schema>
</schemalist>